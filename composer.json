{
    "name": "hua5p/hyperf-logstash",
    "description": "Hyperf Logstash integration package for centralized logging",
    "version": "0.0.7",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "hyperf",
        "logstash",
        "logging",
        "elk",
        "elasticsearch"
    ],
    "authors": [
        {
            "name": "Hua5p Team",
            "email": "dev@hua5p.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "hyperf/framework": "^3.0",
        "hyperf/logger": "^3.0",
        "hyperf/redis": "^3.0",
        "hyperf/process": "^3.0",
        "hyperf/di": "^3.0",
        "monolog/monolog": "^3.0",
        "ramsey/uuid": "^4.0"
    },
    "require-dev": {
        "hyperf/testing": "^3.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Hua5p\\HyperfLogstash\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hua5p\\HyperfLogstash\\Tests\\": "tests/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "config/autoload"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}